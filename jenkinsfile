  agent any
    stages{
      stage('Build'){
          steps{
          echo 'Building Project Hello World !'
               }
             }
      stage('Test'){
        steps{
        echo 'Testing Project Hello World'
             }
            }
       stage('Deploy'){
       steps{
       echo 'Deploying Hello World'
            }
           }
           
           
           }
          }
